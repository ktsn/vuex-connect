{
  "name": "vuex-connect",
  "version": "2.2.0",
  "author": "katashin",
  "description": "A binding utility for a Vue component and a Vuex store.",
  "keywords": [
    "vue",
    "vuex",
    "helper",
    "flux",
    "binding",
    "container"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "license": "MIT",
  "main": "dist/vuex-connect.common.js",
  "module": "dist/vuex-connect.esm.js",
  "unpkg": "dist/vuex-connect.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/ktsn/vuex-connect",
  "bugs": "https://github.com/ktsn/vuex-connect/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vuex-connect.git"
  },
  "scripts": {
    "prepublishOnly": "run-s clean build",
    "clean": "rm -rf dist lib",
    "build": "run-s build:ts build:rollup",
    "build:ts": "tsc -p .",
    "build:rollup": "node scripts/build.js",
    "lint": "eslint \"@(src|test|scripts)/**/*.js\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-preset-power-assert": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-config-ktsn-typescript": "^2.0.0",
    "jest": "^25.1.0",
    "jest-config-ktsn": "^0.2.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.1",
    "prettier": "^2.0.5",
    "prettier-config-ktsn": "^1.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.3",
    "testdouble": "^3.8.2",
    "typescript": "^3.1.3",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^1.0.21 || ^2.0.0",
    "vuex": "^2.0.0 || ^3.0.0"
  }
}
