{
  "name": "vuex-connect",
  "version": "2.1.1",
  "author": "katashin",
  "description": "A binding utility for a Vue component and a Vuex store.",
  "keywords": [
    "vue",
    "vuex",
    "helper",
    "flux",
    "binding",
    "container"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/vuex-connect.common.js",
  "module": "dist/vuex-connect.esm.js",
  "unpkg": "dist/vuex-connect.js",
  "homepage": "https://github.com/ktsn/vuex-connect",
  "bugs": "https://github.com/ktsn/vuex-connect/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vuex-connect.git"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist .tmp",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "lint": "eslint --fix \"@(src|test|scripts)/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "run-s lint test clean build"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-power-assert": "^2.0.0",
    "cross-env": "^5.1.5",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-config-ktsn": "^1.0.3",
    "jest": "^23.6.0",
    "jest-config-ktsn": "^0.1.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "testdouble": "^3.8.1",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^1.0.21 || ^2.0.0",
    "vuex": "^2.0.0 || ^3.0.0"
  }
}
